<% if @student.errors.blank? %>
  $('#dialog_student').modal('toggle');
  $('#students').DataTable().ajax.reload();
  $("#flash_messages").html("<%= escape_javascript(render 'shared/flash_messages') %>");
  console.log("<%= @student.roll_no %> <%= @student.name %> created");
  <% else %>
	$('#dialog_student').modal('show');
<% end %>
