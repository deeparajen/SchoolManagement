<% if @grade.errors.blank? %>
  $('#myModal_grade').modal('toggle');
  $('#grades').DataTable().ajax.reload();
  $("#flash_messages").html("<%= escape_javascript(render 'shared/flash_messages') %>");
  console.log("<%= @grade.grade_name %>  created");
<% else %>
	$('#myModal_grade').modal('show');
<% end %>